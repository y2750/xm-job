<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.FreelancerCertificateMapper">

    <insert id="insert" parameterType="com.example.entity.FreelancerCertificate" useGeneratedKeys="true" keyProperty="id">
        insert into freelancer_certificate
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="freelancerId != null">freelancer_id,</if>
            <if test="certificateType != null">certificate_type,</if>
            <if test="certificateName != null">certificate_name,</if>
            <if test="issuingOrganization != null">issuing_organization,</if>
            <if test="issueDate != null">issue_date,</if>
            <if test="expiryDate != null">expiry_date,</if>
            <if test="certificateUrl != null">certificate_url,</if>
            <if test="verified != null">verified,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="freelancerId != null">#{freelancerId},</if>
            <if test="certificateType != null">#{certificateType},</if>
            <if test="certificateName != null">#{certificateName},</if>
            <if test="issuingOrganization != null">#{issuingOrganization},</if>
            <if test="issueDate != null">#{issueDate},</if>
            <if test="expiryDate != null">#{expiryDate},</if>
            <if test="certificateUrl != null">#{certificateUrl},</if>
            <if test="verified != null">#{verified},</if>
        </trim>
    </insert>

    <update id="updateById" parameterType="com.example.entity.FreelancerCertificate">
        update freelancer_certificate
        <set>
            <if test="certificateType != null">certificate_type = #{certificateType},</if>
            <if test="certificateName != null">certificate_name = #{certificateName},</if>
            <if test="issuingOrganization != null">issuing_organization = #{issuingOrganization},</if>
            <if test="issueDate != null">issue_date = #{issueDate},</if>
            <if test="expiryDate != null">expiry_date = #{expiryDate},</if>
            <if test="certificateUrl != null">certificate_url = #{certificateUrl},</if>
            <if test="verified != null">verified = #{verified},</if>
        </set>
        where id = #{id}
    </update>

    <delete id="deleteById">
        delete from freelancer_certificate where id = #{id}
    </delete>

</mapper>














